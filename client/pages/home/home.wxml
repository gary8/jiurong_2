<swiper indicator-dots="{{indicatorDots}}" 
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="true"> 
      <block wx:for="{{arr}}" wx:key="i"> 
        <swiper-item> 
            <image src="{{item}}" mode='aspectFill' class="slide-image"/> 
        </swiper-item> 
      </block> 
</swiper>